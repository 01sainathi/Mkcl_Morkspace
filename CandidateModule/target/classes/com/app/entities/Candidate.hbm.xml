<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.app.entities">
	
	<class name="Candidate" table="CANDIDATE">
		<id name="cid" column="CID">
			<generator class="native"/>
		</id>
		<property name="name" column="NAME"></property>
		<property name="username" column="UNAME"></property>
		<property name="pass" column="PASSWORD"></property>
		<property name="desc" column="DESCRIPTION"></property>
		<property name="gender" column="GENDER"></property>
		<property name="standard" column="STANDARD"></property>
	</class>
	
</hibernate-mapping>