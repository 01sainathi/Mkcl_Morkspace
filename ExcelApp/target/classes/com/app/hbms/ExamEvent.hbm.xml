<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.app.model">
	<class name="ExamEvent" table="EXAMEVENT">
		<id name="examEventID" column="EXAMEVENTID">
			<generator class="identity"/>
		</id>
		<property name="defaultLanguageID" column="DEFAULTLANGUAGEID"/>
		<property name="examEventCode" column="EXAMEVENTCODE"/>
		<property name="name" column="NAME"/>
	</class>
</hibernate-mapping>